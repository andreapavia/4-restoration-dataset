{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["node_modules/@types"],
    "allowJs": true,
    "types": ["node"],
    "baseUrl": "./",
    "outDir": "dist",
    "moduleResolution": "Node"
  }
}
